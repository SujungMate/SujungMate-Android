<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout_signup3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E3E3F0"
    tools:context=".SignUpActivity3">


    <Spinner
        android:id="@+id/major_spinner_signup3"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginStart="54dp"
        android:layout_marginTop="128dp"
        android:layout_marginEnd="54dp"
        android:layout_marginBottom="130dp"
        android:background="#ffffff"
        android:entries="@array/interesting_major"
        android:paddingLeft="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/next_button_signup3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/line2_view_signup3"
        app:layout_constraintVertical_bias="0.083"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/passwordconfirm_textView_signup3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="56dp"
        android:text="비밀번호 확인"
        android:textColor="@color/black"
        android:textSize="17dp"
        app:layout_constraintStart_toStartOf="@+id/passwordconfirm_edittext_signup3"
        app:layout_constraintTop_toBottomOf="@+id/textView26" />

    <EditText
        android:id="@+id/password_edittext_signup3"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginTop="85dp"
        android:layout_marginEnd="54dp"
        android:layout_marginRight="54dp"
        android:layout_marginBottom="85dp"
        android:background="#ffffff"
        android:hint="@string/prompt_password"
        android:inputType="text"
        android:paddingLeft="8dp"
        android:selectAllOnFocus="true"
        app:layout_constraintBottom_toBottomOf="@+id/passwordconfirm_edittext_signup3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView26"
        app:layout_constraintTop_toBottomOf="@+id/nickname_textView_signup3"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="@string/text_signupwarning"
        android:textColor="@color/red"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar_signup" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="@string/text_star"
        android:textColor="@color/red"
        app:layout_constraintBaseline_toBaselineOf="@+id/nickname_textView_signup3"
        app:layout_constraintEnd_toStartOf="@+id/nickname_textView_signup3" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:text="@string/text_star"
        android:textColor="@color/red"
        app:layout_constraintBottom_toTopOf="@+id/passwordconfirm_edittext_signup3"
        app:layout_constraintEnd_toStartOf="@+id/line2_view_signup3"
        app:layout_constraintStart_toStartOf="@+id/line2_view_signup3" />

    <TextView
        android:id="@+id/nickname_textView_signup3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="6dp"
        android:text="닉네임"
        android:textColor="@color/black"
        android:textSize="17dp"
        app:layout_constraintBottom_toTopOf="@+id/nickname_edittext_signup3"
        app:layout_constraintEnd_toStartOf="@+id/textView19"
        app:layout_constraintStart_toStartOf="@+id/textView19" />

    <Button
        android:id="@+id/next_button_signup3"
        android:layout_width="84.6sp"
        android:layout_height="61sp"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="100dp"
        android:backgroundTint="#7075B6"
        android:text="@string/action_next"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="42dp"
        android:layout_marginLeft="42dp"
        android:layout_marginTop="228dp"
        android:layout_marginEnd="7dp"
        android:layout_marginRight="7dp"
        android:text="@string/text_star"
        android:textColor="@color/red"
        app:layout_constraintEnd_toStartOf="@+id/password_edittext_signup3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/line1_view_signup3"
        android:layout_width="0dp"
        android:layout_height="0.1dp"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="69dp"
        android:layout_marginEnd="44dp"
        android:layout_marginRight="44dp"
        android:layout_marginBottom="69dp"
        android:background="#ADADAD"
        app:layout_constraintBottom_toBottomOf="@+id/password_edittext_signup3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/nickname_textView_signup3" />

    <EditText
        android:id="@+id/nickname_edittext_signup3"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginStart="54dp"
        android:layout_marginLeft="54dp"
        android:layout_marginTop="53dp"
        android:layout_marginEnd="54dp"
        android:layout_marginRight="54dp"
        android:layout_marginBottom="52dp"
        android:background="#ffffff"
        android:hint="@string/prompt_name"
        android:inputType="text"
        android:paddingLeft="8dp"
        android:selectAllOnFocus="true"
        app:layout_constraintBottom_toBottomOf="@+id/textView26"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView19"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/password_textView_signup3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginBottom="1dp"
        android:text="비밀번호"
        android:textColor="@color/black"
        android:textSize="17dp"
        app:layout_constraintBottom_toTopOf="@+id/password_edittext_signup3"
        app:layout_constraintStart_toStartOf="@+id/password_edittext_signup3" />

    <EditText
        android:id="@+id/passwordconfirm_edittext_signup3"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginStart="54dp"
        android:layout_marginLeft="54dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="54dp"
        android:layout_marginRight="54dp"
        android:layout_marginBottom="26dp"
        android:background="#ffffff"
        android:hint="@string/prompt_passwordconfirm"
        android:inputType="text"
        android:paddingLeft="8dp"
        android:selectAllOnFocus="true"
        app:layout_constraintBottom_toTopOf="@+id/major_textView_signup3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/passwordconfirm_textView_signup3"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <View
        android:id="@+id/line3_view_signup3"
        android:layout_width="0dp"
        android:layout_height="0.1dp"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="44dp"
        android:layout_marginRight="44dp"
        android:layout_marginBottom="13dp"
        android:background="#ADADAD"
        app:layout_constraintBottom_toTopOf="@+id/major_textView_signup3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/passwordconfirm_edittext_signup3" />

    <TextView
        android:id="@+id/major_textView_signup3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        android:layout_marginLeft="6dp"
        android:layout_marginTop="59dp"
        android:text="주전공"
        android:textColor="@color/black"
        android:textSize="17dp"
        app:layout_constraintStart_toStartOf="@+id/major_spinner_signup3"
        app:layout_constraintTop_toBottomOf="@+id/passwordconfirm_textView_signup3" />

    <View
        android:id="@+id/line2_view_signup3"
        android:layout_width="0dp"
        android:layout_height="0.1dp"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="44dp"
        android:layout_marginRight="44dp"
        android:layout_marginBottom="16dp"
        android:background="#ADADAD"
        app:layout_constraintBottom_toTopOf="@+id/textView25"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/password_edittext_signup3" />


    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_star"
        android:textColor="@color/red"
        app:layout_constraintBaseline_toBaselineOf="@+id/major_textView_signup3"
        app:layout_constraintEnd_toStartOf="@+id/line3_view_signup3"
        app:layout_constraintStart_toStartOf="@+id/line3_view_signup3" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar_signup"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#F6F6F6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="@string/text_signup"
            android:textColor="@color/black"
            android:textSize="20sp"
            tools:layout_editor_absoluteX="145dp"
            tools:layout_editor_absoluteY="14dp" />

    </androidx.appcompat.widget.Toolbar>


</androidx.constraintlayout.widget.ConstraintLayout>
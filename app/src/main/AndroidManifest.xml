<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sujungmate">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/sujung_mate_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.SujungMate">
        <activity
            android:name=".messages.NewMessagesActivity"
            android:exported="true" />
        <activity
            android:name=".SignUpActivity2_1"
            android:exported="true" />
        <activity
            android:name=".LoginActivity"
            android:exported="true"
            android:usesCleartextTraffic="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MateTypeActivity"
            android:exported="true" />
        <activity
            android:name=".MateInfoActivity"
            android:exported="true" />
        <activity
            android:name=".MateSearchActivity"
            android:exported="true">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MateManageActivity" />
        </activity>

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@drawable/drawble_right_image_customise" />

        <activity
            android:name=".messages.ChatActivity"
            android:exported="true">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".messages.ChatManageActivity" />
        </activity>
        <activity
            android:name=".messages.ChatManageActivity"
            android:exported="true">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MyPageActivity" />
        </activity>
        <activity
            android:name=".SignUpActivity4"
            android:exported="true" />
        <activity
            android:name=".SignUpActivity3"
            android:exported="true" />
        <activity android:name=".SignUpActivity2" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".MyPageActivity"
            android:exported="true"/>
        <activity
            android:name=".InfoModifyActivity"
            android:exported="true" />
        <activity android:name=".PWChangeActivity"
            android:exported="true" />
        <activity android:name=".MateManageActivity"
            android:exported="true">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MyPageActivity" />
        </activity>
    </application>

</manifest>